<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="379.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameUI.GameController">
   <children>
      <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" prefHeight="184.0" prefWidth="1247.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="label2" alignment="CENTER" contentDisplay="CENTER" prefHeight="32.0" prefWidth="600.0" />
      <TextField fx:id="textfield1" alignment="CENTER" onKeyPressed="#handleKey" prefHeight="25.0" prefWidth="334.0">
         <VBox.margin>
            <Insets left="200.0" right="200.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER" prefHeight="64.0" prefWidth="583.0">
         <children>
            <Button fx:id="guess" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleGuess" text="Guess">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="giveup" mnemonicParsing="false" onAction="#handleGiveup" text="Give up" />
         </children>
      </HBox>
   </children>
</VBox>
